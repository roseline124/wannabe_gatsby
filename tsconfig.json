{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "images/*": ["src/assets/images/*"]
    },
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "public", ".cache"]
}
