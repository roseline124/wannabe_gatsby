{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "jsx": "react",
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": false,
        "noUnusedLocals": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules",
        "public",
        ".cache"
    ]
}